  <property name="styleSheet">
   <string notr="true">* {
    font-family: &quot;Microsoft YaHei&quot;, sans-serif;
    font-size: 13px;
}

/* Header label styling */
#signinHeaderLabel {
    width: 300px;
    height: 44px;
    background-color: transparent;
    color: #333333;
	font-size: 16px;
}

/* Input field styling */
#signinUsernameEdit,
#signinPasswordEdit {
    width: 271px;
    height: 38px;
    padding: 2px 25px 2px 2px;
    border-radius: 4px;
    border: 1px solid #7f8ba8;
    background-color: #ffffff;
    color: #999999;
    text-align: left;
}

/* Input fields when disabled */
#signinUsernameEdit:disabled,
#signinPasswordEdit:disabled {
    background-color: #f0f0f0;
}

/* Sign-in button styling */
#signinBtn {
    width: 298px;
    height: 42px;
    border-radius: 2px;
	border: 1px solid #006ffe;
    background-color: #006ffe;
    color: #ffffff;
}

/* Sign-in button hover effect */
#signinBtn:hover {
    background-color: #4093ff;
}

/* Sign-in button pressed effect */
#signinBtn:pressed {
    background-color: #0063e5;
}

/* Charge button styling */
#chargeBtn {
    width: 298px;
    height: 42px;
    border-radius: 2px;
    border: 1px solid #cfd5dc;
    background-color: #ffffff;
    color: #606e80;
}

/* Charge button hover effect */
#chargeBtn:hover {
    color: #2785ff;
}

/* Charge button active effect */
#chargeBtn:pressed {
    color: #006fff;
}

/* Sign-up button styling */
#signupBtn {
    width: 60px;
    height: 16px;
    border: 0;
    background-color: transparent;
    text-decoration: underline;
    color: #2359ff;
	font-size: 12px;
}

/* Sign-up button when disabled */
#signupBtn:disabled {
    color: #909399;
}

/* Change password button styling */
#changePasswordBtn {
    width: 60px;
    height: 16px;
    border: 0;
    background-color: transparent;
    color: #606266;
    font-size: 12px;
}

/* Change password button when disabled */
#changePasswordBtn:disabled {
    color: #909399;
}

#rememberPasswordCheckBox {
 	font-size: 12px;
}</string>
